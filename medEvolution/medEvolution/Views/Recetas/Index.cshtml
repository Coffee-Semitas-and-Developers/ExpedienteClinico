@model IEnumerable<MedEvolution.Models.App.Receta>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdReceta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Consulta.Sintomatología)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medicamento.NombreMedicamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Instrucciones)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdReceta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Consulta.Sintomatología)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medicamento.NombreMedicamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Instrucciones)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdReceta }) |
            @Html.ActionLink("Details", "Details", new { id = item.IdReceta }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdReceta })
        </td>
    </tr>
}

</table>
