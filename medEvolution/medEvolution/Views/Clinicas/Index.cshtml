@model IEnumerable<medEvolution.Models.App.Clinica>

@{
    ViewBag.Title = "Clínicas";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nueva Clínica", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdClinica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreClinica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Colonia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pasaje_Calle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdClinica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreClinica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Colonia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pasaje_Calle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdClinica }) |
            @Html.ActionLink("Details", "Details", new { id = item.IdClinica }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdClinica })
        </td>
    </tr>
}

</table>
